<div class = "test">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="formtext">
    <%= f.label :email %><br />
    <%= f.email_field :email %>
  </div>

  <div class="formtext">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="formtext">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
</div>
